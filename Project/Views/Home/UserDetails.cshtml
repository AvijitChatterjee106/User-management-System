@model Project.Models.UserClass;
@{
    ViewData["Title"] = "UserDetails";
}

<html>

<head>
     <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
     
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
   

	<script src="~/lib/jquery/wdrproduct.js"></script>
	<script src="~/lib/jquery/getall.js"></script>
	<script src="~/lib/jquery/isrej.js"></script>
	@* <script src="~/lib/jquery/aprrej.js"></script> *@
</head>

<body>
    
    <div class="container">
        <h1 class="text-center">All Users Details</h1>
        <a href="/Home/Login" class="btn btn-danger float-end logout">Log Out</a>
        <br />
        <hr />
        <div class="row">
            <div class="col-sm-1"></div>
            <div class="col-sm-10" id="alldata">
                <table class='table table-responsive p-2 ' id='alluserdata'>

                </table>
            </div>
            <div class="col-sm-1"></div>
        </div>

    </div>
    <div class="container">
        <h1 class="text-center">Product List</h1>
        <hr />
        <div class="row">
            <div class="col-sm-1"></div>
            <div class="col-sm-10" >
                <table class='table table-responsive p-2 ' id='allproduct'>

                </table>
            </div>
            <div class="col-sm-1"></div>
        </div>

    </div>

    <div class="container">
        <h1 class="text-center">Deleted/Withdrawal
            Product List</h1>
        <hr />
        <div class="row">
            <div class="col-sm-1"></div>
            <div class="col-sm-10">
                <table class='table table-responsive p-2 ' id='wdrproduct'>
                </table>
            </div>
            <div class="col-sm-1"></div>
        </div>

    </div>

    <div class="container">
        <h1 class="text-center">
            Approved
            Product List
        </h1>
        <hr />
        <div class="row">
            <div class="col-sm-1"></div>
            <div class="col-sm-10">
                <table class='table table-responsive p-2 ' id='aprproduct'>
                </table>
            </div>
            <div class="col-sm-1"></div>
        </div>

    </div>

    <div class="container">
        <h1 class="text-center">
            Rejected
            Product List
        </h1>
        <hr />
        <div class="row">
            <div class="col-sm-1"></div>
            <div class="col-sm-10">
                <table class='table table-responsive p-2 ' id='rejproduct'>
                </table>
            </div>
            <div class="col-sm-1"></div>
        </div>

    </div>
    <br /><br /><br />
</body>
</html>